# custom aliases
alias xpwd='pwd | xclip -sel clip'
alias chrome='google-chrome &> /dev/null &'
alias gui='xdg-open . &> /dev/null &'
alias sgui='sudo bash -c "xdg-open . &> /dev/null &"'
alias gopen='gio open'
## editor
alias fcode='code -n .'
alias sfcode='sudo code -n . --user-data-dir ~/.config/Code/'
alias scode='sudo code -n . --user-data-dir ~/.config/Code/'
alias ccode='code ~/Documents/clipboard'
alias acode='code ~/Documents/develop/lab/algorithm'
## directories
alias develop='cd ~/Documents/develop/'
### openwisp
alias openwisp='cd ~/Documents/develop/openwisp/'
### ansible-*
alias aopenwisp='cd ~/Documents/develop/openwisp/ansible-openwisp'
### openwisp-*
alias ocontroller='cd ~/Documents/develop/openwisp/openwisp-controller && pipenv shell'
alias oconfig='cd ~/Documents/develop/openwisp/openwisp-config'
### django-*
alias dradius='cd ~/Documents/develop/openwisp/django-freeradius && pipenv shell'
alias dipam='cd ~/Documents/develop/openwisp/django-ipam && pipenv shell'
# end custom aliases
